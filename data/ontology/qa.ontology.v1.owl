<owl:Class rdf:ID="سفينة">
  <rdfs:label xml:lang="AR">سفينة</rdfs:label>
  <rdfs:label xml:lang="EN">ship</rdfs:label>
  <annot:frequency xml:lang="EN">1</annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
