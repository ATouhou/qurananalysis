  <annot:meaning_wordnet_translated_ar xml:lang="AR">يملك أو يمارس تأثيرات خبيثة</annot:meaning_wordnet_translated_ar>
  <annot:dbpedia_link xml:lang="EN">http://live.dbpedia.org/resource/Evil</annot:dbpedia_link>
