  <annot:is_qurana_ngram_concept xml:lang="EN"></annot:is_qurana_ngram_concept>
  <annot:synonym_1 xml:lang="EN">sunshine</annot:synonym_1>
  <annot:synonym_2 xml:lang="AR">الشمس</annot:synonym_2>
</الضوء>
