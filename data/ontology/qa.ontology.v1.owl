<الشخص rdf:ID="محمد">
  <rdfs:label xml:lang="AR">محمد</rdfs:label>
  <rdfs:label xml:lang="EN">muhammad</rdfs:label>
  <annot:frequency xml:lang="EN">4</annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
