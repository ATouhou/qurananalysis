<شيء rdf:ID="صيام">
  <rdfs:label xml:lang="AR">صيام</rdfs:label>
  <rdfs:label xml:lang="EN">Fasting</rdfs:label>
  <annot:frequency xml:lang="EN">9</annot:frequency>
