<owl:Class rdf:ID="عيسى">
  <rdfs:label xml:lang="AR">عيسى</rdfs:label>
  <rdfs:label xml:lang="EN">jesus</rdfs:label>
  <annot:frequency xml:lang="EN">25</annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
