<owl:Class rdf:ID="قمر">
  <rdfs:label xml:lang="AR">قمر</rdfs:label>
  <rdfs:label xml:lang="EN">moon</rdfs:label>
  <annot:frequency xml:lang="EN">27</annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
