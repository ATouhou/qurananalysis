  <rdfs:label xml:lang="AR">النفس</rdfs:label>
  <rdfs:label xml:lang="EN">Soul</rdfs:label>
  <annot:frequency xml:lang="EN"></annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
  <annot:pos xml:lang="EN">SUBJECT</annot:pos>
  <annot:transliteration xml:lang="EN">ayyatuh&lt;u&gt;a&lt;/u&gt;</annot:transliteration>
  <annot:lemma xml:lang="AR"></annot:lemma>
  <annot:root xml:lang="AR"></annot:root>
