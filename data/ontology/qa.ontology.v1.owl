<شيء rdf:ID="السوء">
  <rdfs:label xml:lang="AR">السوء</rdfs:label>
  <rdfs:label xml:lang="EN">Evil</rdfs:label>
  <annot:frequency xml:lang="EN"></annot:frequency>
  <annot:weight xml:lang="EN"></annot:weight>
