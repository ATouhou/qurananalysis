  <annot:weight xml:lang="EN"></annot:weight>
  <annot:pos xml:lang="EN"></annot:pos>
  <annot:transliteration xml:lang="EN">nafsin</annot:transliteration>
  <annot:lemma xml:lang="AR">نَفْس</annot:lemma>
  <annot:root xml:lang="AR">نفس</annot:root>
